registration){registration.unregister().then(function(){window.location.reload();});});}else{// Service worker found. Proceed as normal.\nregisterValidSW(swUrl,config);}}).catch(function(){console.log('No internet connection found. App is running in offline mode.');});}function unregister(){if('serviceWorker'in navigator){navigator.serviceWorker.ready.then(function(registration){registration.unregister();});}}\n// CONCATENATED MODULE: ./src/index.js\nreact_dom_default.a.render(/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(src_App,{}),document.getElementById(\"root\"));// If you want your app to work offline and load faster, you can change\n// unregister() to register() below. Note this comes with some pitfalls.\n// Learn more about service workers: https://bit.ly/CRA-PWA\nunregister();\n\n/***/ }),\n\n/***/ 79:\n/***/ (function(module, exports, __webpack_require__) {\n\n// extracted by mini-css-extract-plugin\n\n/***/ })\n\n},[[121,1,2]]]);","inputSourceMap":{"version":3,"sources":["/Users/saitoshi/Documents/GitHub/kinniku-banker/node_modules/mini-css-extract-plugin/dist/loader.js??ref--5-oneOf-4-0!/Users/saitoshi/Documents/GitHub/kinniku-banker/node_modules/css-loader/dist/cjs.js??ref--5-oneOf-4-1!/Users/saitoshi/Documents/GitHub/kinniku-banker/node_modules/postcss-loader/src/index.js??postcss!/Users/saitoshi/Documents/GitHub/kinniku-banker/src/index.css","/Users/saitoshi/Documents/GitHub/kinniku-banker/src/components/NavBar.jsx","/Users/saitoshi/Documents/GitHub/kinniku-banker/src/components/Home.jsx","/Users/saitoshi/Documents/GitHub/kinniku-banker/src/components/Who.jsx","/Users/saitoshi/Documents/GitHub/kinniku-banker/src/App.js","/Users/saitoshi/Documents/GitHub/kinniku-banker/src/serviceWorker.js","/Users/saitoshi/Documents/GitHub/kinniku-banker/src/index.js","/Users/saitoshi/Documents/GitHub/kinniku-banker/node_modules/mini-css-extract-plugin/dist/loader.js??ref--5-oneOf-4-0!/Users/saitoshi/Documents/GitHub/kinniku-banker/node_modules/css-loader/dist/cjs.js??ref